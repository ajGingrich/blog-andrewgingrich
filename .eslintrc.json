{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-unreachable": 2,
        "curly": ["error", "multi-line"],
        "no-console": "error",
        "no-empty": "error",
        "no-compare-neg-zero": "error",
        "no-constant-condition": "error"
    },
    "env": {},
    "parser": "babel-eslint"
}
