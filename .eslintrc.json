{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-unreachable": 2,
        "curly": ["error", "multi-line"],
        "no-console": "error",
        "no-empty": "error",
        "no-compare-neg-zero": "error",
        "no-constant-condition": "error",
        "no-trailing-spaces": [2, { "skipBlankLines": true }],
        "no-var": "error",
        "no-extra-parens": ["error", "all", { "nestedBinaryExpressions": false }]
    },
    "env": {},
    "parser": "babel-eslint"
}
