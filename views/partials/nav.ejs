      <ul class="dropdown-menu pull-right">
        <li><a href="/auth/facebook" class="btn btn-primary btn-facebook"><span class="fa fa-facebook"></span> Facebook</a></li>
        <li><a href="/auth/twitter" class="btn btn-info"><span class="fa fa-twitter"></span> Twitter</a></li>
        <li><a href="/auth/google" class="btn btn-danger"><span class="fa fa-google-plus"></span> Google</a></li>
      </ul>
    </div>
  </nav>
    <% } %>
  <h5><a href="https://github.com/ajGingrich/Pinterest-Replica" target="_blank">github repo</a></h5>
