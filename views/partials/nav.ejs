<div class="container-fluid nav-container">
  <h4>Andrew's Pinterest Replica</h4>
    <% if (login) { %>
  <nav>
    <a class="menu" href="/">Home</a>
    <p>|</p>
    <a class="menu" href="/allUsers">All Users</a>
    <p>|</p>
    <a class="menu" href="/myPics">My Pictures</a>
    <p>|</p>
    <a class="menu" href="#myModal" data-toggle="modal">New Picture</a>
    <p>|</p>
    <a class="menu" href="/logout">Logout</a>
  </nav>
    <% } else { %>
  <nav>
    <a class="menu" href="/">Home</a>
    <p>|</p>
    <a class="menu" href="/allUsers">All Users</a>
    <p>|</p>
